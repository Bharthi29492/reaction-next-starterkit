{
  "presets": [
    [ "next/babel", {
      "preset-env": {
        "targets": {
          "node": "current",
          "browsers": [
            "last 2 versions",
            "ie 11"
          ]
        }
      }
    } ]
  ],
  "plugins": [
    "babel-plugin-styled-components",
    [ "module-resolver", { "root": [ "./src", "./config" ] } ],
    [ "@babel/plugin-proposal-decorators", { "legacy": true } ],
    [ "@babel/plugin-proposal-class-properties", { "loose": true } ],
    "@babel/plugin-proposal-optional-chaining"
  ],
  "env": {
    "jesttest": {
      "plugins": [
        "dynamic-import-node",
        "babel-plugin-styled-components",
        [ "module-resolver", { "root": [ "./src", "./config" ] } ],
        [ "@babel/plugin-proposal-decorators", { "legacy": true } ],
        [ "@babel/plugin-proposal-class-properties", { "loose": true } ],
        "@babel/plugin-proposal-optional-chaining"
      ]
    }
  }
}
